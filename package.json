{
  "name": "gatsby-plugin-intl",
  "description": "Gatsby plugin to add react-intl onto a site",
  "version": "0.1.2",
  "author": "Daewoong Moon <wiziple@gmail.com>",
  "bugs": {
    "url": "https://github.com/wiziple/gatsby-plugin-intl"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "browser-lang": "^0.0.1",
    "intl": "^1.2.5",
    "react-intl": "^2.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3"
  },
  "homepage": "https://github.com/wiziple/gatsby-plugin-intl",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-intl",
    "i18n",
    "intl",
    "react-intl"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "repository": "https://github.com/wiziple/gatsby-plugin-intl",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "format": "prettier --write src/**/*.{js,jsx}",
    "test": "jest",
    "clean": "rimraf *.js"
  }
}
